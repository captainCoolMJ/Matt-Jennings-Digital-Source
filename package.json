{
  "name": "matt-jennings-digital",
  "author": "Matt Jennings",
  "description": "Matt Jennings' Portfolio Site",
  "version": "3.0.0",
  "main": "index.js",
  "scripts": {
    "build:client": "webpack --env.production=true",
    "build:server": "webpack --config=webpack.server.config.js --env.production=true",
    "build": "npm run build:client && npm run build:server",
    "serve": "node public/server.js --port 8080 --public ./public --assets ./public/assets",
    "run": "npm run build && npm run serve"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "jquery": "^1.11.0",
    "jquery.easing": "^1.3.2",
    "terser-webpack-plugin": "^1.3.0",
    "webpack": "^4.38.0",
    "webpack-cli": "^3.3.6",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "minimist": "^1.2.0"
  }
}
